<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>The Backlog</title>
</head>
<body>
    <h1>Game Backlog</h1>
    <ul>
        <% games.forEach(function(g) { %>
            <li>
                <%= g.game %>
                -
                <%= g.game ? 'played':'not played'  %>
            </li>
        <% }); %>
    </ul>
</body>
</html>